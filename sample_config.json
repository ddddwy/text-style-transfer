{
  "training": {
    "optimizer": "adadelta",
    "learning_rate": 1.0,
    "max_norm": 2.0,
    "epochs": 70,
    "batches_per_report": 1,
    "batches_per_sampling": 500,
    "random_seed": 1
  },
  "data": {
    "src": "data/yelp/sentiment.train.1",
    "tgt": "data/yelp/sentiment.train.0",
    "src_test": "data/yelp/sentiment.test.1",
    "tgt_test": "data/yelp/sentiment.test.0",
    "src_vocab": "data/yelp/dict.20k",
    "tgt_vocab": "data/yelp/dict.20k",
    "share_vocab": true,
    "attribute_vocab": "data/yelp/dict_att.20k",
    "batch_size": 256,
    "max_len": 50,
    "working_dir": "sample_run"
  },
    "model": {
        "model_type": "delete_retrieve",
        "emb_dim": 128,
        "attention": false,
        "encoder": "lstm",
        "src_hidden_dim": 512,
        "src_layers": 1,
        "bidirectional": true,
        "tgt_hidden_dim": 512,
        "tgt_layers": 1,
        "decode": "greedy",
        "dropout": 0.2
    }
}